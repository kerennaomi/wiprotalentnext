#program 1

import java.util.*;

public class iseven {
    public static void main(String[] args){
        Scanner input= new Scanner(System.in);
        int n= input.nextInt();
        if(n%2==0)
            System.out.println("Even");
    }
}

#program 2

import java.util.*;

public class isodd {
    public static void main(String[] args){
        Scanner input= new Scanner(System.in);
        int n= input.nextInt();
        if(n%2!=0)
            System.out.println("Odd");
    }
}
