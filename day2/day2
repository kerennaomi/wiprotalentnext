#program1
array_1 = list( input().split(" "))
print(max(array_1))

#program2
array_1 = list(input().split(" "))
maxval = max(array_1)
print(array_1.count(maxval))

#program3
def fact(n):
    factval = 1

    for i in range(1, n+1):
        factval = factval * i
    print(factval)

n = int(input())
fact(n)


#program4
from collections import OrderedDict


def removedup(str):
    return "".join(OrderedDict.fromkeys(str))


input1 = input()
print(removedup(input1))
